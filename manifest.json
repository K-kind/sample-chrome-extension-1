{
  "manifest_version": 3,
  "name": "ツイッターアイコン復元",
  "version": "1.1",
  "description": "ツイッターのアイコンを元に戻します",
  "content_scripts": [
    {
      "js": ["script.js"],
      "matches": ["https://*.twitter.com/*"]
    }
  ],
  "permissions": [
    "contextMenus"
  ],
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }  
}
